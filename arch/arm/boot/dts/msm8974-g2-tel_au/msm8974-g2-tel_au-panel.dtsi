&soc {

	qcom,mdss_dsi_lgit_video {
		status = "disable";
		qcom,cont-splash-enabled;
	};

	qcom,mdss_dsi_lgit_cmd {
		status = "disable";
	};

	qcom,mdss_dsi_jdi_cmd {
		status = "disable";
	};

	qcom,mdss_dsi_jdi_video {
		status = "disable";
	};

	i2c@f9967000 {
		lm3630_rev_b@38 {
			status = "ok";
			lm3630,enable_pwm = <1>;
			revision = "...rev_b";
		};

		lm3630_rev_c@38 {
			status = "ok";
			lm3630,enable_pwm = <1>;
			revision = "rev_c...";
		};
	};
};
